<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.HeroMapper">

  <select id="selectAll" resultType="com.example.demo.dto.Hero">
    select * from heros;
  </select>
  <insert id="insert" useGeneratedKeys="true" keyProperty="id">
  	INSERT INTO heros (id, name, powerName, powerActivate, power, typeBan) 
  		VALUES (#{id}, #{name}, #{powerName}, #{powerActivate}, #{power}, #{typeBan})
  </insert>
</mapper>